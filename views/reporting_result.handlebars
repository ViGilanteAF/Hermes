        <div id="page-wrapper">
            <div class="container-fluid">
                <form action="reporting_result.html" method="post">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header"> 훈련 결과 비교  </h1>

                        <div class="col-lg-6">
                            <label>훈련 비교 대상 1</label>
                            <select class="form-control" name='train1'>
                                <option value=1>훈련을 선택해주세요.</option>
                                {{#each tmp}}
                                <option value="{{this.No}}">{{this.Name}}</option>
                                {{/each}}
                            </select>
                        </div>
                        <div class="col-lg-6">
                            <label>훈련 비교 대상 2</label>
                            <select class="form-control" name='train2'>
                                <option value=2>훈련을 선택해주세요.</option>
                                {{#each tmp}}
                                <option value="{{this.No}}">{{this.Name}}</option>
                                {{/each}}
                            </select>
                        </div>
                        <div class="col-lg-12"><p></p>
                        <button type="submit" class="btn btn-outline btn-primary btn-lg btn-block">훈련 비교 시작</button>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                </form>
                <h1 class="page-header"></h1>
                <div class="row">
                    
                    <div class="col-lg-12">
                        <table class="table">
                          <thead>
                            <tr>
                              <th scope="col"></th>
                              <th scope="col">훈련 1</th>
                              <th scope="col">훈련 2</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr>
                              <th scope="row">훈련명</th>
                              <td>{{#each tmp5}}{{this.Name}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Name}}{{/each}}</td>
                            </tr>
                            <tr>
                              <th scope="row">훈련유형</th>
                              <td>{{#each tmp5}}{{this.Train_Type2}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Train_Type2}}{{/each}}</td>
                            </tr>
                            <tr>
                              <th scope="row">훈련 총 인원</th>
                              <td>{{#each tmp5}}{{this.Train_TotalPeo}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Train_TotalPeo}}{{/each}}</td>
                            </tr>
                            <tr>
                              <th scope="row">훈련 시작일</th>
                              <td>{{#each tmp5}}{{this.Start_Time}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Start_Time}}{{/each}}</td>
                            </tr>                        
                            <tr>
                              <th scope="row">훈련 종료일</th>
                              <td>{{#each tmp5}}{{this.Finish_Time}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Finish_Time}}{{/each}}</td>
                            </tr>
                              <th scope="row">회사명</th>
                              <td>{{#each tmp5}}{{this.Comp_Name}}{{/each}}</td>
                              <td>{{#each tmp6}}{{this.Comp_Name}}{{/each}}</td>
                          </tbody>
                        </table>
                    </div>
                    <div class="col-lg-6">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                보안 점수 비교 (점) <span style="float: right"><p class="text-muted"><small>Higher is Better</small></p></span>
                            </div>
                                <div id="chart">
                                <script>
                                    var chart = c3.generate({
                                        data: {
                                            columns: [
                                                ['훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}', {{#each tmp3}}{{this.TrainPoint1}}{{/each}}],
                                                ['훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}', {{#each tmp4}}{{this.TrainPoint2}}{{/each}}],
                                                ['전체 평균 ', {{#each tmp2}}{{this.TotalPoint}}{{/each}}]
                                            ],
                                            type: 'bar',
                                            labels: true,
                                            colors: {
                                            '훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}': '#be514e',
                                            '훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}': '#4f81c0',
                                            '전체 평균 ': '#99cc5c'
                                            },
                                        },
                                        grid: {
                                            y: {
                                                lines: [{value: 0}]
                                            }
                                        },
                                        bindto: '#chart'
                                    });
                                </script>
                                </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                               공격 성공률 비교 (%) <span style="float: right"><p class="text-muted"><small>Lower is Better</small></p></span>
                            </div>
                                <div id="chart2">
                                <script>
                                    var chart2 = c3.generate({
                                        data: {
                                            columns: [
                                                ['훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}', {{#each tmp5}}{{this.TrainAttack}}{{/each}}],
                                                ['훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}', {{#each tmp6}}{{this.TrainAttack}}{{/each}}],
                                                ['전체 평균 ', {{#each tmp2}}{{this.TotalAttack}}{{/each}}]
                                            ],
                                            type: 'bar',
                                            labels: true,
                                            colors: {
                                            '훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}': '#be514e',
                                            '훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}': '#4f81c0',
                                            '전체 평균 ': '#99cc5c'
                                            }
                                        },
                                        grid: {
                                            y: {
                                                lines: [{value: 0}]
                                            }
                                        },
                                        bindto: '#chart2'
                                    });
                                </script>
                                </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                               훈련 비교 
                            </div>
                            <div id="chart3">
                                <script>
                                    var chart = c3.generate({
                                        data: {
                                            columns: [
                                                ['훈련 1 : {{#each tmp5}}{{this.Name}}', {{this.Train_TotalPeo}}, {{this.Train_DelEmPeo}}, {{this.Train_SpamPeo}}, {{this.Train_ClickPeo}}, {{this.AttackPeo}}]{{/each}},
                                                ['훈련 2 : {{#each tmp6}}{{this.Name}}', {{this.Train_TotalPeo}}, {{this.Train_DelEmPeo}}, {{this.Train_SpamPeo}}, {{this.Train_ClickPeo}}, {{this.AttackPeo}}]{{/each}}
                                            ],
                                            type: 'bar',
                                            labels: true,
                                            colors: {
                                            '훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}': '#be514e',
                                            '훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}': '#4f81c0'
                                            }
                                        },
                                        axis: {
                                            x: {
                                                type: 'category',
                                                categories: ['총 인원 수 ','이메일 삭제 ', '스팸 처리 ', '이메일 클릭 ','공격 성공 횟수 ']
                                            },
                                            rotated: true
                                        },
                                        bindto: '#chart3'
                                    });
                                </script>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                               훈련 비교 
                            </div>
                            <div id="chart4">
                                <script>
                                    var chart = c3.generate({
                                        data: {
                                            columns: [
                                                ['훈련 1 : {{#each tmp5}}{{this.Name}}', {{this.DelP}}, {{this.SpamP}}, {{this.ClickP}}, {{this.TrainAttack}}{{/each}}],
                                                ['훈련 2 : {{#each tmp6}}{{this.Name}}', {{this.DelP}}, {{this.SpamP}}, {{this.ClickP}}, {{this.TrainAttack}}{{/each}}],
                                                ['전체 평균 ', {{#each tmp2}}{{this.DelP}}, {{this.SpamP}}, {{this.ClickP}}, {{this.TotalAttack}}{{/each}}]
                                            ],
                                            colors: {
                                            '훈련 1 : {{#each tmp5}}{{this.Name}}{{/each}}': '#be514e',
                                            '훈련 2 : {{#each tmp6}}{{this.Name}}{{/each}}': '#4f81c0',
                                            '전체 평균 ': '#99cc5c'
                                            }
                                        },
                                        axis: {
                                            x: {
                                                type: 'category',
                                                categories: ['이메일 삭제 ', '스팸 처리 ', '이메일 클릭 ','공격 성공 ']
                                            },
                                        },
                                        bindto: '#chart4'
                                    });
                                    setTimeout(function () {
                                        chart.transform('area', '전체 평균 ');
                                    }, 2000);
                                </script>
                            </div>
                        </div>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
            </div>
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->

    <script src="/public/vendor/jquery/jquery.min.js"></script>

